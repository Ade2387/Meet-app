<div class="dashboard-container">
  <div class="profile-container">
    <h3><%= "Welcome to your dashboard #{current_user.username}!"%></h3>

    <div>
      <img id="dashboard-avatar" src="<%= current_user.photo %>" alt="That's you">
    </div>

      <br>
      <div class="profile-text">
      <p><%= "Username: #{current_user.username}" %></p>
      <p><%= "Email: #{current_user.email}" %></p>
      <p><%= "Member since: #{current_user.created_at}" %></p>
      <a href="/users/edit" class="btn btn-primary">Edit profile</a>
      </div>
  </div>

  <%= month_calendar do |date| %>
    <%= date %>
  <% end %>
</div>
