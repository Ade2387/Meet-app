<H1>Index Event page</H1>

<ul>
  <% @events.each do |event| %>
    <li>Meeting: <%= event.description %>
        - duration: <%= event.duration %>
        - start: <%= event.start_at %>
        - end: <%= event.end_at %>
        - created by: <%= event.user.first_name %></li>
        - attendees: <%#= UserEvent.event_id[event.id] %>
  <% end %>
</ul>

<%= link_to "Home", root_path %>
<%= link_to "New Event", new_user_event_path(current_user) %>
